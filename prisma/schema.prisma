generator client {
  provider        = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Player {
  id                        String       @id @default(auto()) @map("_id") @db.ObjectId
  email                     String       @unique
  username                  String
  password                  String
  currentHashedRefreshToken String?
}
